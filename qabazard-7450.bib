	Shorttitle = {Power and {Influence} in {State} {Legislative} {Policymaking}},
	Title = {Power and {Influence} in {State} {Legislative} {Policymaking}: {The} {Interaction} of {Gender} and {Position} in {Committee} {Hearing} {Debates}},
