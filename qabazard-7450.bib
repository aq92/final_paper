	Number = {4},
	Pages = {785--801},
	Title = {Partisan {Polarization} and the {Effect} of {Congressional} {Performance} {Evaluations} on {Party} {Brands} and {American} {Elections}},
	Url = {https://doi.org/10.1177/1065912915601896},
